@page
@model Frontend.Pages.CEntrenador.DetailsModel
@{
    ViewData["Title"]="Detalles Entrenador";
}
<h2>Detalles De Entrenador</h2>
<h3 class="text-danger"> @ViewData["Error"]</h3>

<div class="form-horizontal">
    <hr/>
    <div class="row">
        <div class="col-md-4">
            <form method="post">                

                <div class="form-group">
                    <label asp-for="Entrenador.Documento" class="control-label"></label>
                    <input asp-for="Entrenador.Documento" class="form-control" readonly/>                   
                </div>

                <div class="form-group">
                    <label asp-for="Entrenador.Nombres" class="control-label"></label>
                    <input asp-for="Entrenador.Nombres" class="form-control" readonly/>                    
                </div>

                <div class="form-group">
                    <label asp-for="Entrenador.Apellidos" class="control-label"></label>
                    <input asp-for="Entrenador.Apellidos" class="form-control" readonly/>                    
                </div>

                <div class="form-group">
                    <label asp-for="Entrenador.Genero" class="control-label"></label>
                    <input asp-for="Entrenador.Genero" class="form-control" readonly/>                    
                </div>

                <div class="form-group">
                    <label asp-for="Entrenador.Celular" class="control-label"></label>
                    <input asp-for="Entrenador.Celular" class="form-control" readonly/>                    
                </div>         
                
                <div class="form-group">
                    <label asp-for="Entrenador.Correo" class="control-label"></label>
                    <input asp-for="Entrenador.Correo" class="form-control" readonly/>                                          
                </div>  

                <div class="form-group">
                    <label asp-for="Entrenador.Nacionalidad" class="control-label"></label>
                    <input asp-for="Entrenador.Nacionalidad" class="form-control" readonly/>                                          
                </div>

                <div class="form-group">
                    <label asp-for="Entrenador.Rh" class="control-label"></label>
                    <input asp-for="Entrenador.Rh" class="form-control" readonly/>                                          
                </div>

                <div class="form-group">
                    <label asp-for="Equipo.Nombre" class="control-label">Diriendo a</label>
                    <input asp-for="Equipo.Nombre" class="form-control" readonly/>                                          
                </div>       

                <div class="form-group">                    
                    <a asp-page="Index" class="btn btn-info">Volver</a>
                </div>
            </form>
        </div>
    </div>
</div>




