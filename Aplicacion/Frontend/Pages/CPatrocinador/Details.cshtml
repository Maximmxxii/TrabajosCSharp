@page
@model Frontend.Pages.CPatrocinador.DetailsModel
@{
    ViewData["Title"] = "Detalles de los Patrocinadores ";
}

<h2> Detalles de los Patrocinadores</h2>

<div class="form-horizontal">
    <hr/>
    <div class="row">
        <div class="col-md-4">
            <form method="post">

                <div class="form-group">
                    <label asp-for="Patrocinador.Id" Class="control-label"></label>
                    <input asp-for="Patrocinador.Id" Class="form-control" readonly />
                </div>
                
                <div class="form-group">
                    <label asp-for="Patrocinador.Identificacion" Class="control-label"></label>
                    <input asp-for="Patrocinador.Identificacion" Class="form-control" readonly />
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Nombre"  Class="control-label"></label>
                    <input asp-for="Patrocinador.Nombre" Class="form-control" readonly />
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Marca"  Class="control-label"></label>
                    <input asp-for="Patrocinador.Marca" Class="form-control" readonly />
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.TipoPersona"  Class="control-label"></label>
                    <input asp-for="Patrocinador.TipoPersona" Class="form-control" readonly />
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Direccion"  Class="control-label"></label>
                    <input asp-for="Patrocinador.Direccion" Class="form-control" readonly />
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Telefono"  Class="control-label"></label>
                    <input asp-for="Patrocinador.Telefono" Class="form-control" readonly />
                </div>  

                <div class="form-group">
                    <a asp-page="Index" class="btn btn-info">Inicio</a>
                </div>
                
            </form>
        </div>
    </div>
</div>